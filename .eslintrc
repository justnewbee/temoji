{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"plugins": [
		"react"
	],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"ecmaFeatures": {
		"arrowFunctions": true,
		"blockBindings": true,
		"classes": true,
		"defaultParams": true,
		"destructuring": true,
		"forOf": true,
		"objectLiteralComputedProperties": true,
		"objectLiteralShorthandMethods": true,
		"objectLiteralShorthandProperties": true,
		"spread": true,
		"superInFunctions": true,
		"templateStrings": true
	},
	"globals": {
		"define": true,
		"angular": true
	},
	"rules": {
		"object-shorthand": ["error", "always"],
		"strict": [0, "global"],
		"global-strict": [0, "always"],
		"yoda": 0,
		"eqeqeq": 0,
		"prefer-const": 2,
		"no-const-assign": "error",
		"consistent-return": 0,
		"dot-notation": [2, {
			"allowKeywords": true
		}],
		"max-len": [1, 200, 2, {
			"ignoreComments": true,
			"ignoreUrls": true
		}],
		"no-new": 0,
		"no-undef": 1,
		"no-unused-expressions": 1,
		"no-use-before-define": 0,
		"no-native-reassign": 1,
		"no-return-assign": 1,
		"no-constant-condition": 1,
		"no-caller": 1,
		"no-loop-func": 1,
		"no-console": 1,
		"no-catch-shadow": 2,
		"no-new-require": 0,
		"no-mixed-requires": [0, false],
		"no-path-concat": 0,
		"no-empty": 0,
		"no-extra-strict": 0,
		"no-shadow": 1,
		"no-unused-vars": [1, {
			"vars": "local",
			"args": "after-used"
		}],
		"handle-callback-err": 0,
		"indent": [2, "tab", {
			"SwitchCase": 0
		}],
		"camelcase": [1, {
			"properties": "always"
		}],
		"quotes": [2, "double", "avoid-escape"],
		"brace-style": [2, "1tbs", {
			"allowSingleLine": false
		}],
		"comma-spacing": [2, {
			"before": false,
			"after": true
		}],
		"comma-style": [2, "last"],
		"eol-last": 0,
		"func-names": 0,
		"new-cap": [2, {
			"newIsCap": true
		}],
		"key-spacing": [1, {
			"beforeColon": false,
			"afterColon": true
		}],
		"no-multi-spaces": 2,
		"no-multiple-empty-lines": 0,
		"no-nested-ternary": 0,
		"no-new-object": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 0,
		"no-extra-parens": 1,
		"no-underscore-dangle": 0,
		"one-var": [1, "never"],
		"padded-blocks": [0, "never"],
		"semi": 2,
		"semi-spacing": 1,
		"keyword-spacing": 2,
		"space-before-blocks": 2,
		"space-before-function-paren": [2, "never"],
		"space-infix-ops": 2,
		"spaced-comment": [1, "always", {
			"exceptions": ["*!+-"]
		}],
		"react/display-name": 0,
		"react/jsx-boolean-value": [1, "always"],
		"jsx-quotes": [2, "prefer-double"],
		"react/jsx-no-undef": 1,
		"react/jsx-sort-props": 0,
		"react/jsx-sort-prop-types": 0,
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 2,
		"react/no-did-mount-set-state": [0, "disallow-in-func"],
		"react/no-did-update-set-state": 0,
		"react/no-multi-comp": 0,
		"react/no-unknown-property": 2,
		"react/prop-types": 0,
		"react/react-in-jsx-scope": 1,
		"react/self-closing-comp": 2,
		"react/wrap-multilines": 0,
		"react/sort-comp": [0, {
			"order": [
				"lifecycle",
				"/^on.+$/",
				"/^(get|set)(?!(InitialState$|DefaultProps$|ChildContext$)).+$/",
				"everything-else",
				"/^render.+$/",
				"render"
			]
		}]
	}
}
